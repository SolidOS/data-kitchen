{
  "name": "Data-Kitchen",
  "version": "1.0.0",
  "description": "SolidOS as a stand-alone desktop app",
  "license": "MIT",
  "author": "Tim Berners-Lee",
  "contributors": [
    "Jeff Zucker"
  ],
  "keywords": [
    "solid",
    "linked data"
  ],
  "main": "index.js",
  "scripts": {
    "startServer": "export NODE_TLS_REJECT_UNAUTHORIZED=0 && node_modules/solid-server/bin/solid-test start --config-file ./server/nss-config.json",
    "start": "export NODE_TLS_REJECT_UNAUTHORIZED=0 && electron . start --config-file ./server/nss-config.json",
    "build": "npm run build:clean && npm run build:cjs && npm run build:esm",
    "build:clean": "rm -rf dist && mkdir dist && mkdir dist/cjs && mkdir dist/esm",
    "build:cjs": "babel --plugins @babel/plugin-transform-modules-commonjs src --out-dir dist/cjs",
    "build:esm": "babel src --out-dir dist/esm"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "electron": "^12.0.2",
    "express": "^4.17.1",
    "penny-pod-inspector": "^0.273522919.1113363993",
    "rss-to-json": "^1.1.2",
    "shelljs": "^0.8.4",
    "solid-server": "^5.6.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "babel-core": "6.26.3",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1"
  }
}
